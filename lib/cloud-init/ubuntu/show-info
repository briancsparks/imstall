#!/bin/bash -e

echo "---------------------------------------------------"
echo ""

echo "dollar-at" "$@"

echo ""
echo "---------------------------------------------------"
echo ""

echo "1: |$1|"
echo "2: |$2|"
echo "3: |$3|"
echo "4: |$4|"
echo "5: |$5|"
echo "6: |$6|"
echo "7: |$7|"

echo ""
echo "---------------------------------------------------"
echo ""

echo "ENV"
env | sort

echo ""
echo "---------------------------------------------------"
echo ""

echo "PATH"
echo "$PATH" | tr ':' '\n'

